<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-lazy="false">
	<import class="com.arthurchan35.wcse.model.Page" />
	<import class="com.arthurchan35.wcse.model.Word" />
	<class name="com.arthurchan35.wcse.model.impl.PageImpl" table="PagesStorage_Page">
		<id name="pageId" type="long">
			<generator class="assigned" />
		</id>
		<property name="url" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="description" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<one-to-one access="com.liferay.portal.dao.orm.hibernate.PrivatePropertyAccessor" cascade="save-update" class="com.arthurchan35.wcse.model.PageImageBlobModel" constrained="true" name="imageBlobModel" outer-join="false" />
	</class>
	<class lazy="true" name="com.arthurchan35.wcse.model.PageImageBlobModel" table="PagesStorage_Page">
		<id column="pageId" name="pageId">
			<generator class="foreign">
				<param name="property">com.arthurchan35.wcse.model.impl.PageImpl</param>
			</generator>
		</id>
		<property column="image" name="imageBlob" type="blob" />
	</class>
	<class name="com.arthurchan35.wcse.model.impl.WordImpl" table="PagesStorage_Word">
		<id name="wordId" type="long">
			<generator class="assigned" />
		</id>
		<property name="pageId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="word" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>